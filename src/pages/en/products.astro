---
import MainLayout from "../../layouts/MainLayout.astro";

const products = [
  "First Product",
  "Second Product",
  "Third Product",
  "Fourth Product",
  "Fifth Product",
]
---

<MainLayout title="Product Page">
	<ul class='w-48 text-sm font-medium rounded-lg border bg-teal-700 border-teal-600 text-white'>
    {
      	products.map((product) => (
        	<li class='py-2 px-4 w-full rounded-t-lg border-b border-teal-600'>
    		{ product }
        	</li>
      	))
    }
  	</ul>
</MainLayout>